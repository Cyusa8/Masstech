{
  "buildCommand": "bun run build",
  "outputDirectory": ".react-router/client",
  "installCommand": "bun install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "runtime": "nodejs20.x",
      "includeFiles": ".react-router/server/**"
    }
  }
}

